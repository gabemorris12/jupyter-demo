# Jupyter Notebook Default Style
axes.prop_cycle: cycler('color', ['000f7e', 'ffbf00', 'black', '3296dc', '7e3b00', 'dc6e32'])
axes.grid: True
axes.labelsize: 18
axes.titlesize: 20

figure.dpi: 240
figure.autolayout: True
figure.figsize: 10, 6

font.size: 12
font.family: "Times"

lines.linewidth: 2.0
lines.markersize: 8

text.usetex: True
